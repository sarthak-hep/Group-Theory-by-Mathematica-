Cell[TextData[{
 "We begin with a ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"2", "\[Cross]", "2"}], TraditionalForm], "errors" -> {}, 
     "input" -> "2\\times2", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "7af9c34e-8258-496b-a3de-11d8114c11e1"],
 " matrix representation. First of all, we define the generating matrices and \
then multiply them to obtain all elements of the group."
}], "Text",
 CellChangeTimes->{{3.8978882929846845`*^9, 3.8978883754711733`*^9}, {
  3.897897759295663*^9, 
  3.8978977605137186`*^9}},ExpressionUUID->"4b6d9968-3e65-430a-9ca6-\
98a018ff170a"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"matE", "=", 
   RowBox[{"IdentityMatrix", "[", "2", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"matC3", "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"-", "1"}], "2"], "*", "matE"}], "+", "\[IndentingNewLine]", 
    RowBox[{
     FractionBox[
      RowBox[{"\[Sqrt]", "3"}], "2"], "*", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", " ", 
         RowBox[{"-", "1"}]}], "}"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"1", ",", " ", "0"}], "}"}]}], "}"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"matSigma", "=", 
   RowBox[{"DiagonalMatrix", "[", 
    RowBox[{"{", 
     RowBox[{"1", ",", " ", 
      RowBox[{"-", "1"}]}], "}"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"matC3sq", "=", 
   RowBox[{"matC3", ".", "matC3"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"matC3Sigma", "=", 
   RowBox[{"matC3", ".", "matSigma"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"matC3sqSigma", "=", 
   RowBox[{"matC3sq", ".", "matSigma"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.897885785039914*^9, 3.8978858661042027`*^9}, {

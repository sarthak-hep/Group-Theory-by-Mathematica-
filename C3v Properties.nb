cac8caa477d1"],

Cell[TextData[{
 "We begin with a ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{"4", "\[Cross]", "4"}], TraditionalForm], "errors" -> {}, 
     "input" -> "4\\times4", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "7af9c34e-8258-496b-a3de-11d8114c11e1"],
 " matrix representation. First of all, we define the generating matrices and \
then multiply them to obtain all elements of the group."
}], "Text",
 CellChangeTimes->{{3.8978882929846845`*^9, 3.8978883754711733`*^9}, {
  3.897897759295663*^9, 
  3.8978977605137186`*^9}},ExpressionUUID->"4b6d9968-3e65-430a-9ca6-\
98a018ff170a"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"matE", "=", 
   RowBox[{"IdentityMatrix", "[", "4", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"matC3", "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", "4"], "*", "matE"}], "+", "\[IndentingNewLine]", 
    RowBox[{
     FractionBox[
      RowBox[{"\[Sqrt]", "3"}], "4"], "*", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "1", ",", "1", ",", "0"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"-", "1"}], ",", "0", ",", "0", ",", "1"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"-", "1"}], ",", "0", ",", "0", ",", "1"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"0", ",", 
         RowBox[{"-", "1"}], ",", 
         RowBox[{"-", "1"}], ",", "0"}], "}"}]}], "}"}]}], "+", 
    "\[IndentingNewLine]", 
    RowBox[{
     FractionBox["3", "4"], "*", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0", ",", "0", ",", "1"}], "}"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"0", ",", "0", ",", 
         RowBox[{"-", "1"}], ",", " ", "0"}], "}"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"0", ",", " ", 
         RowBox[{"-", "1"}], ",", " ", "0", ",", " ", "0"}], "}"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"1", ",", " ", "0", ",", " ", "0", ",", " ", "0"}], "}"}]}], 
      "}"}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"matSigma", "=", 
   RowBox[{"DiagonalMatrix", "[", 
    RowBox[{"{", 
     RowBox[{"1", ",", " ", 
      RowBox[{"-", "1"}], ",", " ", 
      RowBox[{"-", "1"}], ",", " ", "1"}], "}"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"matC3sq", "=", 
   RowBox[{"matC3", ".", "matC3"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"matC3Sigma", "=", 
   RowBox[{"matC3", ".", "matSigma"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"matC3sqSigma", "=", 
   RowBox[{"matC3sq", ".", "matSigma"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.897885785039914*^9, 3.8978858661042027`*^9}, {
   3.897886015580321*^9, 3.8978862127650537`*^9}, 3.897886398066974*^9},
 CellLabel->"In[1]:=",ExpressionUUID->"8ecf0301-11b8-4ae0-8d43-302ad045111f"],

